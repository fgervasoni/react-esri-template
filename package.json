{
    "name": "react-esri-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 5000 --open",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
        "prepare": "husky install",
        "cy:open": "cypress open",
        "cy:run": "cypress run --headed",
        "dev:testing": "vite --port 6001 --mode test",
        "cy:test": "concurrently --kill-others \"npm run dev:testing\" \"npm run cy:run\""
    },
    "dependencies": {
        "@arcgis/core": "^4.30.9",
        "i18next": "^23.16.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^15.1.0",
        "react-router-dom": "^6.27.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.0",
        "cypress": "^13.15.1",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react-hooks": "^5.0.0",
        "globals": "^15.12.0",
        "husky": "^9.1.6",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "tailwindcss": "^3.4.14",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.13.0",
        "vite": "^5.4.10"
    }
}
